{
  "dataform-version": "1.0.0",
  "type": "gridpanel",
  "showLabels": true,
  "children": [
    {
      "type": "selectbox",
      "field": "unit",
      "label": "${unit}",
      "searchAttribute": "name",
      "labelAttribute": "${name}",
      "values": [
        {
          "name": "${unit.meters}",
          "value": "meters"
        },
        {
          "name": "${kilometers}",
          "value": "kilometers"
        },
        {
          "name": "${feet}",
          "value": "feet"
        },
        {
          "name": "${usfeet}",
          "value": "us-feet"
        },
        {
          "name": "${yards}",
          "value": "yards"
        },
        {
          "name": "${miles}",
          "value": "miles"
        },
        {
          "name": "${nauticalmiles}",
          "value": "nautical-miles"
        }
      ],
      "max": 2,
      "size": {
        "w": 100
      }
    }
  ]
}
