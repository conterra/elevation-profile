{
  "properties": {
    "title": "Elevation Profile Sample",
    "amdPackages": "apprt@^3.2.1"
  },
  "load": {
    "bundleLocations": [
      "localbundles",
      "bundles"
    ],
    "allowedBundles": [
      "themes@^3.2.1",
      "templates@^3.2.1",
      "splashscreen@^3.2.1",
      "system@^3.2.1",
      "templatelayout@^3.2.1",
      "map@^3.2.1",
      "notifier@^3.2.1",
      "toolset@^3.2.1",
      "toolrules@^3.2.1",
      "banner@^3.2.1",
      "custominfo@^3.2.1",
      "parametermanager@^3.2.1",
      "dn_elevationprofile"
    ]
  },
  "bundles": {
    "dn_elevationprofile": {
      "ElevationProfileWidgetFactory": {
        "unit": "esriMeters",
        "profileTaskUrl": "https://elevation.arcgis.com/arcgis/rest/services/Tools/ElevationSync/GPServer",
        "geometryServiceUrl": "https://utility.arcgisonline.com/ArcGIS/rest/services/Geometry/GeometryServer",
        "chartOptions": {
          "title": "H\u00f6henprofil",
          "chartTitleFontSize": 12,
          "axisTitleFontSize": 10,
          "axisLabelFontSize": 8,
          "indicatorFontColor": "#eee",
          "indicatorFillColor": "#666",
          "busyIndicatorBackgroundColor": "#666",
          "titleFontColor": "#eee",
          "axisFontColor": "#ccc",
          "axisMajorTickColor": "#333",
          "skyTopColor": "#B0E0E6",
          "skyBottomColor": "#4682B4",
          "waterLineColor": "#eee",
          "waterTopColor": "#ADD8E6",
          "waterBottomColor": "#0000FF",
          "elevationLineColor": "#D2B48C",
          "elevationTopColor": "#8B4513",
          "elevationBottomColor": "#CD853F",
          "elevationMarkerStrokeColor": "#FF0000",
          "elevationMarkerSymbol": "m -6 -6, l 12 12, m 0 -12, l -12 12"
        },
        "lineOptions": {
          "type": "esriSLS",
          "style": "esriSLSSolid",
          "color": [
            255,
            255,
            0
          ],
          "width": 2
        }
      }
    },
    "banner": {
      "BannerWidget": {
        "label": "Developer Network",
        "image": "https://www.conterra.de/~/media/conterra/images/site_logos/conterra-logo.ashx",
        "imagePosition": "above",
        "link": "http://developernetwork.conterra.de"
      }
    },
    "custominfo": {
      "CustomInfoToggleTool": [
        {
          "id": "customInfoTool",
          "activateOnStartup": true,
          "title": "Information",
          "tooltip": "Information",
          "iconClass": "icon-custom-info-info",
          "toolClass": "ctInfo",
          "togglable": true,
          "widgetSettings": {
            "content": "This app shows the functionality of the Elevation Profile bundle which enables the Elevation Profile Widget from Esri (new in ArcGIS API for JavaScript 3.14 ) in map.apps.<br><br> PLease click on the button in the upper left corner. The new window enables you to either draw a polyline or a freehand geometry. It then calculates an elevation profile along this line.",
            "window": {
              "title": "Information",
              "marginBox": {
                "w": 300,
                "h": 160,
                "l": 25,
                "b": 70
              },
              "draggable": true,
              "closable": true,
              "modal": false,
              "maximizable": false,
              "resizable": false
            }
          }
        }
      ]
    },
    "themes": {
      "ThemeModel": {
        "_selectedTheme": "everlasting"
      },
      "ThemeSelector": {
        "componentEnabled": false
      }
    },
    "templates": {
      "TemplateSelector": {
        "componentEnabled": false
      },
      "enabled": true,
      "TemplateModel": {
        "_selectedTemplate": "seasons"
      }
    },
    "map": {
      "MapModelFactory": {
        "_configData": {
          "maps": [
            {
              "baseLayer": [
                {
                  "id": "streets",
                  "title": "Streets",
                  "service": "streets",
                  "layers": [
                    "*"
                  ]
                }
              ]
            }
          ]
        }
      },
      "MapState": {
        "initialExtent": {
          "xmin": 600000,
          "ymin": 5800000,
          "xmax": 1700000,
          "ymax": 7600000,
          "spatialReference": {
            "wkid": 3857
          }
        }
      },
      "MappingResourceRegistryFactory": {
        "_knownServices": {
          "services": [
            {
              "id": "streets",
              "type": "AGS_TILED",
              "url": "http://services.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer"
            }
          ]
        }
      },
      "enabled": true
    },
    "measurement": {
      "MeasurementFactory": {
        "showTools": [
          "area",
          "distance",
          "location"
        ],
        "defaultTool": "distance",
        "defaultAreaUnit": "SQUARE_KILOMETERS",
        "defaultLengthUnit": "KILOMETERS",
        "defaultLocationUnit": "DECIMAL_DEGREES",
        "finishWithButton": true
      },
      "enabled": true
    },
    "navigationtoolset": {
      "enabled": true
    },
    "notifier": {
      "enabled": true
    },
    "overviewmap": {
      "enabled": true
    },
    "parametermanager": {
      "enabled": true
    },
    "scalebar": {
      "enabled": true
    },
    "templatelayout": {
      "enabled": true
    },
    "toolrules": {
      "enabled": true
    },
    "toolset": {
      "ToolsetManager": {
        "toolsets": [
          {
            "id": "toolset_1437464739023",
            "title": "Werkzeuge",
            "container": "map",
            "windowType": "fixed",
            "window": {
              "closable": true
            },
            "tooltip": "Werkzeuge",
            "cssClass": "notitle",
            "__isDirty": true,
            "max_horizontal": 1,
            "position": {
              "rel_l": 20,
              "rel_t": 20
            },
            "tools": [
              "elevationProfileTool",
              "overviewmap",
              "zoomToFullExtentTool",
              "zoomToPrevExtentTool",
              "coordinateFinderTool",
              "measurementtool"
            ]
          }
        ]
      },
      "enabled": true
    },
    "coordinatefinder": {
      "enabled": true
    }
  }
}